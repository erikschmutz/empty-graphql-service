{
  "name": "empty-graphql-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "_moduleAliases": {
    "@server": "dist/server",
    "@utils": "dist/utils",
    "@interfaces": "dist/interfaces",
    "@models": "dist/models",
    "@assets": "dist/assets",
    "@controllers": "dist/controllers",
    "src": "dist"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src",
    "start:dev": "nodemon",
    "build": "rimraf dist && tsc --project tsconfig.json && gulp",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migrate:run": "npm run migrate migration:run",
    "migrate:generate": "node scripts/migrate:generate.js",
    "migrate:create": "npm run migrate migration:create",
    "docker:dev": "node scripts/docker:dev.js",
    "docker:reset": "node scripts/docker:reset.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:integration": "INTER=TRUE jest --detectOpenHandles --forceExit",
    "test:lint": "eslint './src/**/*.{ts,tsx}' --max-warnings 0",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "git:pre-push": "npm run test && npm run test:lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/federation": "^0.21.0",
    "apollo-server-core": "^2.21.0",
    "apollo-server-express": "^2.21.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fix": "0.0.6",
    "fs-extra": "^9.1.0",
    "graphql": "^15.5.0",
    "inversify": "^5.0.5",
    "json5": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.1.5"
  }
}
